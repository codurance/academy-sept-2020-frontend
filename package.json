{
  "name": "react-skeleton",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "put here your repository",
  "author": "put here the authors",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack serve --config webpack.config.dev.js",
    "test": "jest",
    "lint": "npx eslint . --ext .js",
    "prettier": "npx prettier --write 'src/**/*.(jsx|js|scss)'",
    "test-coverage": "jest --collect-coverage"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-google-login": "^5.1.25",
    "react-router-dom": "^5.2.0",
    "react-use-googlelogin": "^0.12.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@testing-library/dom": "^7.28.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.1",
    "@testing-library/user-event": "^12.2.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "babel-plugin-transform-scss": "^1.0.9",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "lint-staged": "^10.5.1",
    "mini-css-extract-plugin": "^1.3.1",
    "miragejs": "^0.1.41",
    "node-sass": "^5.0.0",
    "prettier": "^2.1.2",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.5.1",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "eslintConfig": {
    "settings": {
      "react": {
        "version": "16.14.0"
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "prettier/react",
      "plugin:jsx-a11y/recommended",
      "plugin:react-hooks/recommended",
      "plugin:import/errors"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "plugins": [
      "prettier",
      "react",
      "jsx-a11y",
      "react-hooks",
      "import"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn build",
      "pre-push": "yarn test && yarn build"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "yarn test --ci --bail --findRelatedTests --watchAll=false",
      "webpack --mode production"
    ]
  }
}
